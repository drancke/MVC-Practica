



<strong><h4>Registro de Proyectos</h4></strong>
<hr />

@{
    ViewBag.Title = "IndexProyecto";
}

<ul class="nav nav-tabs">
    <li class="active"><a data-toggle="tab" href="#firstTab">Ver Proyectos</a></li>
    <li><a data-toggle="tab" href="#secondTab">Crear Nuevo Proyecto</a></li>
</ul>
<div class="tab-content">
    <div id="firstTab" class="tab-pane fade in  active">@Html.Action("VerProyectos")</div>
    <div id="secondTab" class="tab-pane fade in">@Html.Action("AgregarOrEditarFormProyecto")</div>
</div>

<link href="https://cdn.datatables.net/1.10.16/css/jquery.dataTables.min.css" rel="stylesheet" />
@section scripts{
    @Scripts.Render("~/bundles/jqueryval")

    <script src="https://cdn.datatables.net/1.10.16/js/jquery.dataTables.min.js"></script>


    <script>
        function activatejQueryTable() {
            $("#ProyectoTable").DataTable();
        }

        $(function () {
            activatejQueryTable();
        });

    </script>
}