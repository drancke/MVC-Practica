@model IEnumerable<Loguate.Models.FormProyecto>

@{
    ViewBag.Title = "VerProyectos";
    Layout = null;
}


<div class="borde">
    <br />
    <div style="overflow-x:auto;">
        <table class="table table-striped table-condensed" id="ProyectoTable">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.Nombre)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Tipo)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Version)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Herramientas)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Clientes)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Empresas)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.AreaModulo)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Productos)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Consultor_Levantamiento)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Fecha_Levantamiento)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Fecha_inicio_desarrollo)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.fecha_calidad)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.comentario)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Consultor_Dev)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Fecha_Entrega)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Atachar_Archivo)
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Nombre)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Tipo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Version)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Herramientas)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Clientes)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Empresas)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.AreaModulo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Productos)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Consultor_Levantamiento)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Fecha_Levantamiento)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Fecha_inicio_desarrollo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.fecha_calidad)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.comentario)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Consultor_Dev)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Fecha_Entrega)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Atachar_Archivo)
                        </td>
                        <td>
                            <a class="btn btn-primary btn-sm" onclick="Edit('@Url.Action("AgregarOrEditarFormProyecto", "ReporteForm", new { id=@item.ID_Proyectos})')"><i class="fa fa-pencil fa-lg"></i></a>
                            <a class="btn btn-danger btn-sm" onclick="Delete('@Url.Action("DeleteForm", "ReporteForm", new { id=@item.ID_Proyectos})')"><i class="fa fa-trash fa-lg"></i></a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>

    </div>
    <br /> 
    <br /> 
    
</div>

